    "@types/react": "^18.2.25",
    "@types/react-datepicker": "^4.15.1",
    "@types/react-dom": "^18.2.10",
    "concurrently": "^7.6.0",
    "cypress": "^15.6.0",
    "esbuild": "^0.27.3",
    "esbuild-node-externals": "^1.9.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "start-server-and-test": "^2.0.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^4.9.5",
