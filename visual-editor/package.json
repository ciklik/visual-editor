{
  "name": "@boxraiser/visual-editor",
  "version": "0.0.10",
  "type": "module",
  "main": "VisualEditor.js",
  "types": "./VisualEditor.d.ts",
  "scripts": {
    "dev": "concurrently \"vite\" \"php -S 0.0.0.0:8000 -ddisplay_errors=1 -t server/\"",
    "build": "tsc && vite build && node esbuild.js && npm run declaration && cp package.json dist/package.json",
    "serve": "vite preview",
    "format": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write",
    "declaration": "tsc --declaration --emitDeclarationOnly --jsx react-jsx --esModuleInterop --target es2020 -p tsconfig.json --noEmit false && tsc-alias",
    "test-watch": "start-server-and-test 'php -S 0.0.0.0:8000 -ddisplay_errors=1 -q' 8000 'npx cypress open'",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "start-server-and-test 'php -S 0.0.0.0:8000 -ddisplay_errors=1 -q' 8000 'npx cypress run'",
    "test:e2e:watch": "start-server-and-test 'php -S 0.0.0.0:8000 -ddisplay_errors=1 -q' 8000 'npx cypress open'",
    "test:unit": "jest"
  },
